{
  "name": "moova/larapi-fast",
  "description": "Package to fast generate Resources in Larapi distribution. Very small, very simple but works.",
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "1.6.*"
  },
  "autoload": {
    "psr-4": {
      "Versoo\\LarapiFast\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Versoo\\LarapiFast\\Provider\\LaravelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Marcin Halak",
      "email": "marcin@halak.pl"
    }
  ]
}
